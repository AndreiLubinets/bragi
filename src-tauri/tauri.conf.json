{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "productName": "Bragi",
  "mainBinaryName": "Bragi",
  "version": "0.1.2",
  "identifier": "com.bragi",
  "plugins": {},
  "app": {
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "Bragi",
        "width": 800,
        "height": 400,
        "theme": "Dark"
      }
    ],
    "security": {
      "csp": null
    }
  }
}
